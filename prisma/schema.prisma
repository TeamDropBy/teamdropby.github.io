generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id        Int      @id @default(autoincrement())
  title     String
  details   String
  contact   String
  createdAt DateTime @default(now())

  user_id   String   // UUID of the Supabase user
}